


class ContextManager:
    def __enter__(self):
        print('Вызов метода __enter__()')
        return self
    
    def __exit__(self, exc_type, exc_value, traceback):
        print('Вызов метода __exit__()')

    def __str__(self):
        return 'ContextManager'


with ContextManager() as cm:
    print(cm)






